<?xml version="1.0" encoding="UTF-8" ?>
<configuration debug="true">
	<appender name="CONSOLE" class="ch.qos.logback.core.ConsoleAppender">
		<encoder>
			<pattern>
				%-5level %logger - %msg%n
			</pattern>
		</encoder>
	</appender>

	<logger name="demo" level="WARN" />

	<root level="ERROR">
		<appender-ref ref="CONSOLE" />
	</root>
</configuration>  
